[{"name": "Ripple planet", "link": "https://github.com/VKLowe-Original/Ripple_planet", "desc": "Russian modding", "icon": "images/ripple_planet-icon.png", "icon_raw": "sprites/blocks/turrets/ripplya.png", "stars": 2, "author": "VKLowe & DefenseX", "date_tt": 1579297299.0, "repo": "VKLowe-Original/Ripple_planet", "contents": ["liquids", "mechs", "items", "blocks", "units"], "assets": ["content", "sprites", "sprites-override"], "displayName": null, "issue": null, "readme": "# Ripple_planet\n* \u041f\u043e\u0447\u0442\u0430: vklowe@petruchio.org.ru\n* \u041e VKLowe: https://petruchio.org.ru/wiki/index.php/VKLowe\n* \u041f\u043e\u043c\u043e\u0433\u0430\u0435\u0442 \u0441 \u043c\u043e\u0434\u043e\u043c: https://github.com/DefenseX\n", "version": "0.3", "wiki": null, "date": "2020-01-17 16:41:39", "header": "", "delta_ago": "5 hours"}, {"name": "thorium-yeeter", "link": "https://github.com/tcookiem/thorium-yeeter1", "desc": "A balancing mod that adds the Thorium Yeeter.", "icon": null, "icon_raw": "", "stars": 0, "author": "tcookiem", "date_tt": 1579289144.0, "repo": "tcookiem/thorium-yeeter1", "contents": ["blocks"], "assets": ["content", "sprites"], "displayName": "[yellow]T[pink]h[yellow]o[pink]r[yellow]i[pink]u[yellow]m [pink]Y[yellow]e[pink]e[yellow]t[pink]e[yellow]r [white]Mod", "issue": null, "readme": "# Test Mod\nA test mod I've made for the game Mindustry, for fun. Not intended to actually be used.\n\n\n## Features\n- **Thorium Yeeter**\n\n  Advanced but early-game crafting block that synthesizes thorium from copper.\n\n  Input: 2 copper\n\n  Output: 1 thorium\n\n  Build cost: 100 copper\n", "version": "1.0", "wiki": null, "date": "2020-01-17 14:25:44", "header": "", "delta_ago": "7 hours"}, {"name": "DustryX", "link": "https://github.com/Xusk947/DustryX", "desc": "Hello ^^ , thanks for playinglink to mod discord Server https://discord.gg/7xpBEJJ", "icon": "images/dustryx-icon.png", "icon_raw": "sprites/units/machinegunner.png", "stars": 4, "author": "Xusk", "da